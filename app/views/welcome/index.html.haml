      \=========================
      PRE LOADER
      \==============================
    .preloader
      .status
        .loader
          %svg.circular{:viewbox => "25 25 50 50"}
            %circle.path{:cx => "50", :cy => "50", :fill => "none", :r => "20", "stroke-miterlimit" => "10", "stroke-width" => "3"}
    /
      \=========================
      SECTION: HOME / HOMEPAGE
      \==============================
    %section#home.home.fixed-bg
      .color-overlay
        / COLOR OVERLAY
        / NAVIGATION
        .navbar.navbar-inverse.bs-docs-nav.navbar-fixed-top.sticky-navigation{:role => "navigation"}
          .container
            .navbar-header
              / NAVBAR EXPAND COLLAPSE ON MOBILE
              %button.navbar-toggle{"data-target" => "#alexis-navigation", "data-toggle" => "collapse", :type => "button"}
                %span.sr-only Toggle navigation
                %span.icon.icon-menu
              / LOGO
              %a.navbar-brand{:href => "#"}
                %img{:alt => "LOGO", :src => "assets/logo.png"}/
            #alexis-navigation.navbar-collapse.collapse
              / NAVIGATION LINK
              %ul#internal-scroll.nav.navbar-nav.navbar-right.main-navigation
                %li
                  %a{:href => "#home"} Home
                %li
                  %a{:href => "#section1"} About
                %li
                  %a{:href => "#section2"} Services
                %li
                  %a{:href => "#section3"} Portfolio
                %li
